
@model BidDetail

<form asp-action=""
<div class="mb-3 row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Номер заявки</label>
    <div class="col-sm-10">
        <input type="text" class="form-control-plaintext" id="staticEmail" value=@Model?.BidId>
    </div>
</div>
<div class="mb-3 row">
    <label for="staticEmail" class="col-sm-2 col-form-label">ФИЮ пользователя</label>
    <div class="col-sm-10">
        <input type="text" class="form-control-plaintext" id="staticEmail" value=@Model?.FullNameUser>
    </div>
</div>
<div class="mb-3 row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Email</label>
    <div class="col-sm-10">
        <input type="text" class="form-control-plaintext" id="staticEmail" value=@Model?.Email>
    </div>
</div>
<div class="mb-3 row">
    <label for="staticEmail" class="col-sm-2 col-form-label">Имя организация</label>
    <div class="col-sm-10">
        <input type="text" class="form-control-plaintext" id="staticEmail" value=@Model?.NameOrganization>
    </div>
</div>